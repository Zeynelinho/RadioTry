<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.zeynelinho.gatherradiotry.view.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_favoriesFragment"
            app:destination="@id/favoriesFragment" />
    </fragment>
    <fragment
        android:id="@+id/playerFragment"
        android:name="com.zeynelinho.gatherradiotry.view.PlayerFragment"
        android:label="fragment_player"
        tools:layout="@layout/fragment_player" />
    <fragment
        android:id="@+id/favoriesFragment"
        android:name="com.zeynelinho.gatherradiotry.view.FavoriesFragment"
        android:label="FavoriesFragment" />
</navigation>